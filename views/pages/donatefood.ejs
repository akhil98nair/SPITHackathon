<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>

<body>
    <header>
        <%- include('../partials/header.ejs') %>
    </header>
    <section id="donatefood">
        <div>
            <form>
                <div class="control-group">
                    <div class="form-group floating-label-form-group controls mb-0 pb-1">

                        <input class="form-control" id="food_description" name="food_description" type="text"
                            placeholder="Food Description" required="required">
                        <p class="help-block text-danger"></p>
                    </div>
                </div>

                <div class="control-group">
                    <div class="form-group floating-label-form-group controls mb-0 pb-1">

                        <input class="form-control" id="location" name="Locaton" type="text" placeholder="Locaton"
                            required="required">
                        <p class="help-block text-danger"></p>
                    </div>
                </div>

                <div class="control-group">
                    <div class="form-group floating-label-form-group controls mb-0 pb-1">

                        <input class="form-control btn btn-primary" id="submit" name="submit" type="submit" value="Submit">                          

                    </div>
                </div>
            </form>
        </div>
    </section>

    <footer>
        <%- include('../partials/footer.ejs') %>
    </footer>

</body>

</html>